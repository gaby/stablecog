		"vite": "^4.3.8",
		"yaml": "^2.1.3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.9.4",
		"@supabase/supabase-js": "^2.22.0",
		"@tanstack/svelte-query": "^4.24.6",
