{
	"name": "stablecog",
	"version": "4.01",
	"private": true,
	"scripts": {
		"dev": "npm run i18n & vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"host": "vite --host",
		"start": "export $(xargs <.env) && node ./build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"update-types": "export $(xargs <.env) && npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/lib/ts/types/supabase.ts",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"@sveltejs/kit": "^1.18.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/d3": "^7.4.0",
		"@types/html-to-text": "^8.1.1",
		"@types/intl-unofficial-duration-unit-format": "^3.1.0",
		"@types/mixpanel-browser": "^2.38.0",
		"@types/uuid": "^8.3.4",
		"autoprefixer": "^10.4.10",
		"bezier-easing": "^2.1.0",
		"d3": "^7.8.5",
		"intl-unofficial-duration-unit-format": "^3.1.0",
		"js-base64": "^3.7.5",
		"mdsvex": "^0.10.6",
		"mixpanel-browser": "^2.45.0",
		"postcss": "^8.4.16",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"rehype-external-links": "^2.1.0",
		"rehype-infer-reading-time-meta": "^1.1.0",
		"rehype-slug": "^5.1.0",
		"rehype-toc": "^3.0.2",
		"remark-external-links": "^9.0.1",
		"remark-toc": "^8.0.1",
		"svelte": "^3.59.1",
		"svelte-check": "^2.7.1",
		"svelte-confetti": "^1.2.2",
		"svelte-copy": "^1.2.4",
		"svelte-intersection-observer": "^0.10.0",
